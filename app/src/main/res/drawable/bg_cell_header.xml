<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true">
        <shape>
            <stroke android:width="0.5dp" android:color="@color/timetable_header_border" />
            <solid android:color="@color/timetable_header_bg" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <stroke android:width="0.5dp" android:color="@color/timetable_header_border_disabled" />
            <solid android:color="@color/timetable_header_bg_disabled" />
        </shape>
    </item>
</selector>
